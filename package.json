{
    "name": "eatup",
    "version": "1.0.1",
    "description": "",
    "engines": {
        "node": "4.1.1"
    },
    "main": "./js/index.jsx",
    "scripts": {
        "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:jsx && sass --watch sass:public/css",
        "watch": "npm run watch:html & npm run watch:css & npm run watch:jsx",
        "test": "mocha --compilers js:babel-register test/**/*.js*",
        "start": "nodemon ./data/app.js",
        "mkdir": "mkdir -p public && mkdir -p public/css",
        "clean": "rm -rf public",
        "build:html": "npm run clean:html && cp index.html public/",
        "watch:html": "npm run build:html && chokidar index.html -c 'npm run build:html'",
        "clean:html": "rm -f public/index.html",
        "build:js": "npm run clean:js && webpack",
        "watch:js": "npm run build:js && webpack --watch",
        "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map",
        "clean:css": "rm public/css/main.css",
        "watch:css": "cp -rf assets public/ && sass sass:public/css --watch",
        "build:jsx": "npm run clean:jsx && webpack",
        "watch:jsx": "npm run build:jsx && webpack --watch",
        "clean:jsx": "rm -f build/$npm_package_name.$npm_package_version.jsx build/$npm_package_name.$npm_package_version.jsx.map"
    },
    "author": "Simon Attfield, Connie Jew, Kaeside Iwuagwu",
    "license": "ISC",
    "devDependencies": {
        "babel-core": ">=6.8.0",
        "babel-loader": ">=6.2.4",
        "babel-preset-es2015": ">=6.6.0",
        "babel-preset-react": ">=6.5.0",
        "babel-register": ">=6.8.0",
        "chai": "^3.5.0",
        "chai-http": "^2.0.1",
        "chokidar-cli": "^1.2.0",
        "css-loader": ">=0.23.1",
        "http-server": "^0.9.0",
        "mocha": ">=2.4.5",
        "node-sass": ">=3.7.0",
        "nodemon": ">=1.9.2",
        "react-addons-test-utils": ">=15.0.2",
        "sass-loader": ">=3.2.0",
        "webpack": ">=1.13.0",
        "webpack-dev-server": ">=1.14.1"
    },
    "dependencies": {
        "body-parser": ">=1.15.1",
        "chai": "^3.5.0",
        "express": "^4.13.4",
        "isomorphic-fetch": ">=2.2.1",
        "mocha": "^2.5.3",
        "mongodb": "^2.1.19",
        "mongoose": "^4.4.19",
        "nodemon": "^1.9.2",
        "react": ">=15.1.0",
        "react-addons-update": ">=15.0.2",
        "react-dom": ">=15.0.2",
        "react-redux": ">=4.4.5",
        "react-router": "^2.4.1",
        "redux": ">=3.5.2",
        "redux-thunk": ">=2.1.0",
        "sendgrid": "^2.0.0"
    }
}
